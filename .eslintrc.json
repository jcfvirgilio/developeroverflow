{
  "extends": [
   "eslint:recommended",
    "plugin:react/recommended",
      "next/core-web-vitals",
    "prettier"
  ],
	"rules": {
    "max-len": [2, 250],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ],
    "object-curly-newline": 0
	  }
}